{
  "name": "joshuaanderton/paypal",
  "description": "PayPal Checkout for Laravel + Inertia.js",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1|^8.2"
  },
  "autoload": {
      "psr-4": {
          "Ja\\PayPal\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ja\\PayPal\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Ja\\PayPal\\Providers\\ServiceProvider"
          ]
      }
  }
}
